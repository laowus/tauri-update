{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri-update",
  "version": "0.1.2",
  "identifier": "com.laowus.tauri-update",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "tauri-update",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "createUpdaterArtifacts": true,
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "https://api.github.com/repos/laowus/tauri-update/releases/latest"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDQ3NDMyN0YzMzM5RjcyM0MKUldROGNwOHo4eWREUnlFZ2tEYlB1N1JlclVwQ3RZMFM0RFZLc2dReEsxYWRyZnQ3OG5XalZ3VHcK"
    }
  }
}
