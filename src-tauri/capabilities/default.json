{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main", "updater"],
  "permissions": [
    "core:default",
    "opener:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://*:*"
        },
        {
          "url": "https://*:*"
        },
        {
          "url": "https://github.com/laowus/*"
        }
      ]
    },
    "updater:default"
  ]
}
